{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "./src"
        ],
        "includePattern": "\\.(jsx|js|ts|tsx)$"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/typescript",
        "node_modules/better-docs/component"
    ],
    "templates": {
        "better-docs": {
            "name": "React components"
        }
    },
    "opts": {
        "encoding": "utf8",
        "readme": "README.md",
        "destination": "docs",
        "recurse": true
    }
}